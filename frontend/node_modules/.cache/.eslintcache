cause i can't get rid of the first empty element in array\n                pokemon.name?.length > 0 && \n                <div>\n                  <h2 className={classes.pokename}>\n                    {pokemon.name.toString().charAt(0).toUpperCase() + pokemon.name.toString().slice(1)}\n                  </h2>\n                  <div className={classes.sprite}>\n                    <img src={pokemon.sprite}></img>\n                  </div>\n                  <RemovePokemon className={classes.rembutton} poketeam={poketeam} setPoketeam={setPoketeam} name={pokemon.name} index={index}/>\n                </div>  \n              }\n            </div>\n          ))\n        }\n      </div>\n      <hr />\n      <h3 className={classes.choosepoketitle}>Choose your Pokemon!</h3>\n      <div className={classes.pokelistmain}>\n          <PokemonList \n            poketeam={poketeam} \n            setPoketeam={setPoketeam} \n            pokemonData={ pokemonData } />\n      </div>\n    </div>\n  );\n}\nexport default CreateTeamPage;","/Users/jin/codeplatoon/projects/pokemon/frontend/src/pages/HomePage.js",["69","70","71"],[],"import { useNavigate } from 'react-router-dom'\nimport { Link } from 'react-router-dom'\nimport PokeAPI from '../api/PokeAPI'\nimport classes from \"./HomePage.module.css\"\nfunction HomePage(props){\n\n  const renderAuthItems = () => {\n    if (props.username === \"\") {\n      return (\n      <>\n        <button className={classes.homebuttons}><Link to=\"signup\">Sign Up</Link></button>\n        <button className={classes.homebuttons}><Link to=\"login\">Login to Account</Link></button>\n      </>\n      )\n    }\n  }\n\n  return(\n    <div className={classes.home}>\n      <h2>Gotta catch 'em all!</h2>\n      <p>Sylveon says, \"Log in or sign up to start making your own Pokemon Team!\"</p>\n      <img src=\"https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/official-artwork/700.png\" className={classes.homeimg}></img>\n      <br/>\n      { renderAuthItems() }\n    </div>    \n  );\n}\n\nexport default HomePage;","/Users/jin/codeplatoon/projects/pokemon/frontend/src/api/PokeAPI.js",[],[],"/Users/jin/codeplatoon/projects/pokemon/frontend/src/api/apiHelpers.js",[],[],"/Users/jin/codeplatoon/projects/pokemon/frontend/src/pages/LoginPage.js",["72"],[],"/Users/jin/codeplatoon/projects/pokemon/frontend/src/pages/SignUpPage.js",["73"],[],"import PokeAPI from \"../api/PokeAPI\"\nimport { useNavigate } from 'react-router-dom'\nimport classes from \"./SignUpPage.module.css\"\n\nfunction SignUpPage(props){\n  \n  //router params\n  const navigate = useNavigate()\n\n  //event handler\n  const handleSignUp = async (evt) => {\n    evt.preventDefault()\n\n    let signUpData = {\n      username: evt.target.elements[\"username\"].value,\n      password: evt.target.elements[\"password\"].value,\n    }\n    console.log(\"SIGN UP INFO:\", signUpData)\n    const data = await PokeAPI.signUp(signUpData)\n\n    if(data){\n      navigate(\"/login\") // redirects to home page\n    }\n  }\n\n  return(\n    <div className={classes.signupbody}>\n      <h2>Sign Up Page</h2>\n      <img src=\"https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/dream-world/101.svg\"></img>\n      <p>Voltorb said, \"Can't make a team if you're not signed up!\"</p>\n      <form onSubmit={ handleSignUp } method=\"POST\">\n        <label className={classes.signuplabel}>Username:</label>\n        <input name=\"username\" placeholder=\"enter username\" />\n        <br/>\n        <label className={classes.signuplabel}>Password:</label>\n        <input type=\"password\" name=\"password\" placeholder=\"create a password\" />\n        <br />\n        <button type=\"submit\" className={classes.signupbutton}>Sign Up!</button>\n      </form>\n    </div>\n  )\n}\n\nexport default SignUpPage;","/Users/jin/codeplatoon/projects/pokemon/frontend/src/components/PokemonList.js",["74","75","76","77","78","79"],[],"import PokeAPI from 